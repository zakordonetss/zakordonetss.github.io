: Helvetica, 'Roboto', sans-serif\r\n\r\n.product\r\n    display: none\r\n    position: relative\r\n    max-width: 1440px\r\n    margin: 0 auto\r\n    padding: 60px\r\n\r\n    &__top\r\n        display: flex\r\n        justify-content: space-between\r\n\r\n    &_active\r\n        display: block\r\n\r\n.scrollbar\r\n    display: flex\r\n    flex-direction: column\r\n\r\n    &__link\r\n        display: flex\r\n        height: 166px\r\n        margin: 10px\r\n\r\n        align-items: center\r\n        border: 2px solid #E3E2E2\r\n        box-sizing: border-box\r\n\r\n        cursor: pointer\r\n\r\n        &:hover\r\n            border: 2px solid #33F0A9\r\n    &__img\r\n        max-width: 100%\r\n        max-height: 115px\r\n\r\n.product-photo\r\n    display: flex\r\n    align-items: center\r\n\r\n    &__large-inner\r\n        display: none\r\n        max-width: 539px\r\n        height: 539px\r\n        margin: 10px\r\n        align-items: center\r\n\r\n        border: 2px solid #E3E2E2\r\n        box-sizing: border-box\r\n\r\n        &_active\r\n            display: flex\r\n\r\n    &__large-img\r\n        max-width: 100%\r\n\r\n// PRODUCT OPTIONS ==================================\r\n\r\n.product\r\n    &__options\r\n        width: 40%\r\n        margin: 10px\r\n\r\n    &__label\r\n        font-size: 46px\r\n\r\n    &__var\r\n        margin-top: 35px\r\n        margin-bottom: 50px\r\n\r\n.var\r\n    &__top\r\n        display: flex\r\n        justify-content: space-between\r\n        font-size: 24px\r\n\r\n        border-bottom: 2px solid #000\r\n\r\n    &__label\r\n        line-height: 150%\r\n\r\n    &__price\r\n        line-height: 150%\r\n        position: relative\r\n        margin-left: 3px\r\n\r\n        &_crossed-out::after\r\n            display: block\r\n            position: absolute\r\n            content: \"\"\r\n            right: 0\r\n            top: 50%\r\n            width: 100%\r\n            height: 2px\r\n            background: #D01917\r\n\r\n        &_sale\r\n            color: #D01917\r\n\r\n    &__colors\r\n        margin-top: 18px\r\n\r\n    &__link\r\n        position: relative\r\n        display: inline-block\r\n        width: 54px\r\n        height: 44px\r\n        margin-right: 8px\r\n\r\n        cursor: pointer\r\n\r\n        &:hover .var__img-outer::after\r\n            display: block\r\n\r\n    &__img\r\n        width: 100%\r\n\r\n        &-outer\r\n            transition: all .1s linear\r\n\r\n            &::after\r\n                left: -12px\r\n                bottom: -25px\r\n                display: none\r\n                position: absolute\r\n                content: \"Blue Mirror\"\r\n                font-family: Helvetica, 'Roboto', sans-serif\r\n                width: 80px\r\n            &_sale\r\n                &::before\r\n                    display: block\r\n                    position: absolute\r\n                    content: \"SALE\"\r\n                    right: 0\r\n                    top: 0\r\n                    width: 34px\r\n                    height: 16px\r\n                    line-height: 16px\r\n                    font-size: 10px\r\n                    text-align: center\r\n                    color: #FFFFFF\r\n                    background: #F14237\r\n\r\n// PRODUCT CARDS ==============================================\r\n\r\n.product__info\r\n    margin-right: 30px\r\n\r\n    &-outer\r\n        display: flex\r\n        margin: 20px 10px 10px 10px\r\n\r\n    &-column\r\n        display: flex\r\n\r\n.product__card\r\n    width: 147px\r\n    height: 149px\r\n    text-align: center\r\n\r\n    background: #33F0A9\r\n\r\n    &-label\r\n        font-size: 18px\r\n        height: 52px\r\n        line-height: 52px\r\n\r\n    &-info\r\n        font-family: Helvetica, 'Roboto', sans-serif\r\n        font-size: 16px\r\n        line-height: 140%\r\n        width: 134px\r\n        margin: 0 auto\r\n\r\n// BUTTONS ===================================\r\n\r\n.product\r\n\r\n    &__buttons\r\n        position: absolute\r\n        right: 60px\r\n        bottom: 80px\r\n\r\n    &__button\r\n        margin-top: 15px\r\n\r\n.button\r\n    background-color: #000\r\n\r\n    &_build\r\n        background-color: #33F0A9\r\n    &__btn\r\n        width: 530px\r\n        height: 74px\r\n        padding: 0\r\n        border: none\r\n        font: inherit\r\n        font-size: 16px\r\n        color: inherit\r\n        background-color: #33F0A9\r\n        transition: all .1s linear\r\n\r\n        &:hover\r\n            transform: translateX(-8px) translateY(-10px)\r\n\r\n        &_build\r\n            color: #33F0A9\r\n            background-color: #000\r\n\r\n// MEDIA QUERIES =======================================================\r\n\r\n@media (max-width: 1440px)\r\n\r\n    .button__btn\r\n        width: 450px\r\n\r\n@media (max-width: 1300px)\r\n    .product\r\n        padding: 10px\r\n\r\n        &__top\r\n            flex-direction: column\r\n            align-items: center\r\n\r\n    .product-photo\r\n        &__large-inner\r\n            max-width: 100%\r\n\r\n    .product\r\n        &__options\r\n            width: 98%\r\n\r\n        &__label\r\n            display: inline-block\r\n            margin-top: 30px\r\n\r\n    .product__info\r\n        margin-right: 0\r\n\r\n        &-outer\r\n            justify-content: space-around\r\n            flex-wrap: wrap\r\n\r\n        &-column\r\n            width: 50%\r\n            justify-content: space-around\r\n\r\n    .product\r\n        &__buttons\r\n            position: static\r\n            margin-top: 40px\r\n\r\n        &__button\r\n            margin-top: 15px\r\n\r\n    .button\r\n        width: 90%\r\n        margin: 20px auto\r\n\r\n        &__btn\r\n            width: 100%\r\n\r\n@media (max-width: 670px)\r\n\r\n    .product-photo\r\n        flex-direction: column-reverse\r\n\r\n        &__large-inner\r\n            height: 350px\r\n\r\n    .scrollbar\r\n        flex-direction: row\r\n\r\n        &__link\r\n            height: 110px\r\n\r\n    .product__card\r\n        width: 100px\r\n        height: 120px\r\n\r\n        &-label\r\n            font-size: 14px\r\n            height: 40px\r\n            line-height: 40px\r\n\r\n        &-info\r\n            font-size: 12px\r\n            line-height: 140%\r\n            width: 90px\r\n\r\n@media (max-width: 480px)\r\n\r\n    .product-photo__large-inner\r\n        height: 250px\r\n\r\n    .scrollbar\r\n        flex-direction: row\r\n\r\n        &__link\r\n            height: 80px\r\n\r\n    .product__label\r\n        font-size: 35px\r\n\r\n    .var__top\r\n        font-size: 16px\r\n\r\n    .product__info\r\n        &-outer\r\n            margin-top: -15px\r\n\r\n        &-column\r\n            width: 100%\r\n            margin: 10px 0\r\n\r\n    .product__card\r\n        width: 125px\r\n        height: 135px\r\n\r\n        &-label\r\n            font-size: 16px\r\n            height: 45px\r\n            line-height: 45px\r\n\r\n        &-info\r\n            font-size: 14px\r\n            line-height: 140%\r\n            width: 120px\r\n"},"lineCount":null}},"error":null,"hash":"4eb90cd937eae8307d154ea650b7d88c","cacheData":{"env":{}}}